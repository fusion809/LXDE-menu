[Desktop Action Next]
Exec=rhythmbox-client --next
Name=Next

[Desktop Action PlayPause]
Exec=rhythmbox-client --play-pause
Name=Play/Pause

[Desktop Action Previous]
Exec=rhythmbox-client --previous
Name=Previous

[Desktop Action StopQuit]
Exec=rhythmbox-client --quit
Name=Stop & Quit


   <Filename>Arch-Developers.desktop</Filename>
   <Filename>Arch-Documentation.desktop</Filename>
   <Filename>Arch-Donate.desktop</Filename>
   <Filename>Arch-Forum.desktop</Filename>
   <Filename>Arch-Homepage.desktop</Filename>
   <Filename>Arch-Schwag.desktop</Filename>
   <Filename>Arch-SVN.desktop</Filename>
   <Filename>Arch-Wiki.desktop</Filename>
  </Layout>
  <Include>
   <Filename>Arch-AUR.desktop</Filename>
   <Filename>Arch-Bugs.desktop</Filename>
   <Filename>Arch-Developers.desktop</Filename>
   <Filename>Arch-Documentation.desktop</Filename>
   <Filename>Arch-Donate.desktop</Filename>
   <Filename>Arch-Forum.desktop</Filename>
   <Filename>Arch-Homepage.desktop</Filename>
   <Filename>Arch-SVN.desktop</Filename>
   <Filename>Arch-Schwag.desktop</Filename>
   <Filename>Arch-Wiki.desktop</Filename>
  </Include>
  <Deleted/>
 </Menu>
 <Menu>
  <Name>.hidden</Name>
  <Include>
   <Filename>Terminal-Konsole (Zsh).desktop</Filename>
   <Filename>sage-notebook.desktop</Filename>
  </Include>
 </Menu>
 <Menu>
  <Name>Applications</Name>
  <Exclude>
   <Filename>Arch-AUR.desktop</Filename>
   <Filename>Arch-Bugs.desktop</Filename>
   <Filename>Arch-Developers.desktop</Filename>
   <Filename>Arch-Documentation.desktop</Filename>
   <Filename>Arch-Donate.desktop</Filename>
   <Filename>Arch-Forum.desktop</Filename>
   <Filename>Arch-Homepage.desktop</Filename>
   <Filename>Arch-SVN.desktop</Filename>
   <Filename>Arch-Schwag.desktop</Filename>
   <Filename>Arch-Wiki.desktop</Filename>
  </Exclude>
  <Deleted/>
 </Menu>
 <Layout>
  <Merge type="menus"/>
  <Menuname>Atom</Menuname>
  <Menuname>Development</Menuname>
  <Menuname>Education</Menuname>
  <Menuname>File Managers</Menuname>
  <Menuname>Games</Menuname>
  <Menuname>Graphics</Menuname>
  <Menuname>gVim</Menuname>
  <Menuname>Internet</Menuname>
  <Menuname>Multimedia</Menuname>
  <Menuname>Office</Menuname>
  <Menuname>Settingsmenu</Menuname>
  <Menuname>System</Menuname>
  <Menuname>Terminal Emulators</Menuname>
  <Menuname>Text Editors</Menuname>
  <Menuname>Utilities</Menuname>
  <Menuname>Virtualization Software</Menuname>
  <Merge type="files"/>
  <Filename>kde4-Help.desktop</Filename>
 </Layout>
</Menu>
<!-- End Applications -->
